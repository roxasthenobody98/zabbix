                    <key>travis.builds.total_duration</key>
                    <delay>0</delay>
                    <units>s</units>
                    <applications>
                        <application>
                            <name>Travis</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>JAVASCRIPT</type>
                            <params>var result = JSON.parse(value);
var duration = 0;
for (i in result){
    duration += result[i].duration;
}
return duration</params>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>travis.builds</key>
                    </master_item>
                </item>
