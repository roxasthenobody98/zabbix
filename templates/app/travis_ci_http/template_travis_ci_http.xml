            <discovery_rules>
                <discovery_rule>
                    <name>Repo's builds</name>
                    <type>DEPENDENT</type>
                    <key>travis.builds.lld</key>
                    <delay>0</delay>
                    <lifetime>1d</lifetime>
                    <item_prototypes>
                        <item_prototype>
                            <name>{#SLUG} count of all builds</name>
                            <type>DEPENDENT</type>
                            <key>travis.build.all_builds[{#SLUG}]</key>
                            <delay>0</delay>
                            <value_type>FLOAT</value_type>
                            <application_prototypes>
                                <application_prototype>
                                    <name>{#SLUG} repo</name>
                                </application_prototype>
                            </application_prototypes>
                            <preprocessing>
                                <step>
                                    <type>JAVASCRIPT</type>
                                    <params>return JSON.parse(value).length</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>travis.build.list[{#SLUG}]</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>{#SLUG} average duration of the build</name>
                            <type>DEPENDENT</type>
                            <key>travis.build.avg_duration[{#SLUG}]</key>
                            <delay>0</delay>
                            <application_prototypes>
                                <application_prototype>
                                    <name>{#SLUG} repo</name>
                                </application_prototype>
                            </application_prototypes>
                            <preprocessing>
                                <step>
                                    <type>JAVASCRIPT</type>
                                    <params>result = JSON.parse(value).filter(function (e) { return e.duration != &quot;0&quot; &amp;&amp; e.duration != null });
var duration = 0;
for (i in result) {
   duration += result[i].duration;
}
return Math.round(duration / result.length)</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>travis.build.list[{#SLUG}]</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>{#SLUG} description</name>
                            <type>DEPENDENT</type>
                            <key>travis.build.description[{#SLUG}]</key>
                            <delay>0</delay>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <application_prototypes>
                                <application_prototype>
                                    <name>{#SLUG} repo</name>
                                </application_prototype>
                            </application_prototypes>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$.repos[?(@.slug == &quot;{#SLUG}&quot;)].description.first()</params>
                                </step>
                                <step>
                                    <type>DISCARD_UNCHANGED</type>
                                    <params/>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>travis.repos</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>{#SLUG} last build duration</name>
                            <type>DEPENDENT</type>
                            <key>travis.build.duration[{#SLUG}]</key>
                            <delay>0</delay>
                            <units>s</units>
                            <application_prototypes>
                                <application_prototype>
                                    <name>{#SLUG} repo</name>
                                </application_prototype>
                            </application_prototypes>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$.repos[?(@.slug == &quot;{#SLUG}&quot;)].last_build_duration.first()</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>travis.repos</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>{#SLUG} count of failed builds</name>
                            <type>DEPENDENT</type>
                            <key>travis.build.failed_builds[{#SLUG}]</key>
                            <delay>0</delay>
                            <application_prototypes>
                                <application_prototype>
                                    <name>{#SLUG} repo</name>
                                </application_prototype>
                            </application_prototypes>
                            <preprocessing>
                                <step>
                                    <type>JAVASCRIPT</type>
                                    <params>return JSON.parse(value).filter(function (e){return e.state != &quot;passed&quot; &amp;&amp; e.state != &quot;started&quot; }).length
</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>travis.build.list[{#SLUG}]</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>{#SLUG} count of passed builds</name>
                            <type>DEPENDENT</type>
                            <key>travis.build.failed_passed[{#SLUG}]</key>
                            <delay>0</delay>
                            <application_prototypes>
                                <application_prototype>
                                    <name>{#SLUG} repo</name>
                                </application_prototype>
                            </application_prototypes>
                            <preprocessing>
                                <step>
                                    <type>JAVASCRIPT</type>
                                    <params>return JSON.parse(value).filter(function (e){return e.state == &quot;passed&quot;}).length
</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>travis.build.list[{#SLUG}]</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>{#SLUG} last build id</name>
                            <type>DEPENDENT</type>
                            <key>travis.build.id[{#SLUG}]</key>
                            <delay>0</delay>
                            <application_prototypes>
                                <application_prototype>
                                    <name>{#SLUG} repo</name>
                                </application_prototype>
                            </application_prototypes>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$.repos[?(@.slug == &quot;{#SLUG}&quot;)].last_build_id.first()</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>travis.repos</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>{#SLUG} builds</name>
                            <type>HTTP_AGENT</type>
                            <key>travis.build.list[{#SLUG}]</key>
                            <history>0</history>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <applications>
                                <application>
                                    <name>RAW</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JAVASCRIPT</type>
                                    <params>return JSON.stringify(JSON.parse(value).builds.map(function (e) {
   delete e.config
   delete e.pull_request
   delete e.pull_request_title
   delete e.pull_request_number
   return e
}))</params>
                                </step>
                            </preprocessing>
                            <url>https://{$TRAVIS_API_URL}/repos/{#SLUG}/builds</url>
                            <headers>
                                <header>
                                    <name>Accept</name>
                                    <value>application/vnd.travis-ci.2.1+json</value>
                                </header>
                                <header>
                                    <name>Authorization</name>
                                    <value>token {$TRAVIS_API_TOKEN}</value>
                                </header>
                            </headers>
                        </item_prototype>
                        <item_prototype>
                            <name>{#SLUG} last build number</name>
                            <type>DEPENDENT</type>
                            <key>travis.build.number[{#SLUG}]</key>
                            <delay>0</delay>
                            <application_prototypes>
                                <application_prototype>
                                    <name>{#SLUG} repo</name>
                                </application_prototype>
                            </application_prototypes>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$.repos[?(@.slug == &quot;{#SLUG}&quot;)].last_build_number.first()</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>travis.repos</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>{#SLUG} last build state</name>
                            <type>DEPENDENT</type>
                            <key>travis.build.state[{#SLUG}]</key>
                            <delay>0</delay>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <application_prototypes>
                                <application_prototype>
                                    <name>{#SLUG} repo</name>
                                </application_prototype>
                            </application_prototypes>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$.repos[?(@.slug == &quot;{#SLUG}&quot;)].last_build_state.first()</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>travis.repos</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>{#SLUG} duration of all builds</name>
                            <type>DEPENDENT</type>
                            <key>travis.build.total_duration[{#SLUG}]</key>
                            <delay>0</delay>
                            <units>s</units>
                            <application_prototypes>
                                <application_prototype>
                                    <name>{#SLUG} repo</name>
                                </application_prototype>
                            </application_prototypes>
                            <preprocessing>
                                <step>
                                    <type>JAVASCRIPT</type>
                                    <params>value = JSON.parse(value);
var duration = 0;
for (i in value) {
   duration += value[i].duration;
}
return duration
</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>travis.build.list[{#SLUG}]</key>
                            </master_item>
                        </item_prototype>
                    </item_prototypes>
                    <graph_prototypes>
                        <graph_prototype>
                            <name>{#SLUG} builds</name>
                            <height>300</height>
                            <graph_items>
                                <graph_item>
                                    <sortorder>1</sortorder>
                                    <color>199C0D</color>
                                    <item>
                                        <host>Template Travis CI by HTTP</host>
                                        <key>travis.build.all_builds[{#SLUG}]</key>
                                    </item>
                                </graph_item>
                                <graph_item>
                                    <sortorder>2</sortorder>
                                    <color>F63100</color>
