            <items>
                <item>
                    <name>Accounts count</name>
                    <type>HTTP_AGENT</type>
                    <key>travis.accounts.count</key>
                    <applications>
                        <application>
                            <name>Travis</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>JAVASCRIPT</type>
                            <params>var result = JSON.parse(value).accounts
if (Array.isArray(result)){
   return result.length;
} else {
   return 0;
}</params>
                        </step>
                    </preprocessing>
                    <url>https://{$TRAVIS_API_URL}/accounts</url>
                    <headers>
                        <header>
                            <name>Accept</name>
                            <value>application/vnd.travis-ci.2.1+json</value>
                        </header>
                        <header>
                            <name>Authorization</name>
                            <value>token {$TRAVIS_API_TOKEN}</value>
                        </header>
                    </headers>
                </item>
