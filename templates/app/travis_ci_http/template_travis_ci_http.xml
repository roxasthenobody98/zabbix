                    </applications>
                    <preprocessing>
                        <step>
                            <type>JAVASCRIPT</type>
                            <params>return JSON.stringify(JSON.parse(value).builds.map(function (e) {return {&quot;id&quot;: e.id, &quot;duration&quot;: e.duration}}))</params>
                        </step>
                    </preprocessing>
                    <url>https://{$TRAVIS_API_URL}/builds</url>
                    <headers>
                        <header>
                            <name>Accept</name>
