                                        <key>travis.build.failed_passed[{#SLUG}]</key>
                                    </item>
                                </graph_item>
                            </graph_items>
                        </graph_prototype>
                    </graph_prototypes>
                    <master_item>
                        <key>travis.repos</key>
                    </master_item>
                    <lld_macro_paths>
                        <lld_macro_path>
                            <lld_macro>{#SLUG}</lld_macro>
                            <path>$..slug.first()</path>
                        </lld_macro_path>
                    </lld_macro_paths>
                    <preprocessing>
                        <step>
                            <type>JAVASCRIPT</type>
                            <params>result = JSON.parse(value);
result.data = result.repos;
delete result.repos;
return JSON.stringify(result)</params>
                        </step>
                    </preprocessing>
